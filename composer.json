{
    "name": "tir/crud",
    "description": "Laravel CRUD scaffolding system",
    "license": "MIT",
    "authors": [
        {
            "name": "Tirdad Abbasi",
            "email": "abbasi.tirdad@gmail.com"
        }
    ],
    "require": {
        "laravel/framework": "^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^8.0",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Tir\\Crud\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tir\\Crud\\Tests\\": "tests/",
            "App\\": "tests/App/"
        }
    },

    "extra": {
        "laravel": {
            "providers": [
                "Tir\\Crud\\CrudServiceProvider"
            ],
            "aliases": {
                "Crud": "Tir\\Crud\\Support\\Facades\\Crud"
            }
        }
    }
}
